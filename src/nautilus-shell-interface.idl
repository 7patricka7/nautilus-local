#ifndef NAUTILUS_SHELL_INTERFACE_IDL
#define NAUTILUS_SHELL_INTERFACE_IDL

#include <Bonobo.idl>
#include <libnautilus/nautilus-view-component.idl>

module Nautilus {

	interface Shell : ::Bonobo::Unknown {
		oneway void open_windows (in URIList uris, in string geometry);
		oneway void open_default_window (in string geometry);
		oneway void start_desktop ();
		oneway void stop_desktop ();
		oneway void quit ();
		oneway void restart ();
	};

};

