#.
#: libnautilus-extensions/nautilus-file.c:1753
msgid "yesterday %-I:%M %p"
msgstr "gestern %-H:%M"

#. Current week, include day of week.
#. * Note to localizers: Same issues as "today" string.
#.
#: libnautilus-extensions/nautilus-file.c:1758
msgid "%A %-m/%-d/%y %-I:%M %p"
msgstr "%A, %-d.%-m.%y %-H.%M"

#. Other dates.
