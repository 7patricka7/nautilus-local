<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="NautilusEntryCompletionPopover" parent="GtkPopover">
    <property name="autohide">False</property>
    <property name="can-focus">False</property>
    <property name="has-arrow">False</property>
    <property name="child">
      <object class="GtkScrolledWindow" id="scrolled_window">
        <property name="hscrollbar-policy">external</property>
        <property name="propagate-natural-height">True</property>
        <property name="child">
          <object class="GtkListView" id="list_view">
            <property name="model">
              <object class="GtkSingleSelection" id="selection_model">
                <property name="autoselect">False</property>
                <signal name="items-changed" handler="selection_model_items_changed_cb" swapped="yes"/>
                <signal name="notify::model" handler="selection_model_notify_model_cb" swapped="yes"/>
                <signal name="notify::selected" handler="selection_model_notify_selected_cb" swapped="yes"/>
              </object>
            </property>
            <signal name="notify::factory" handler="list_view_notify_factory_cb" swapped="yes"/>
            <signal name="notify::model" handler="list_view_notify_model_cb" swapped="yes"/>
            <style>
              <class name="entry-completion"/>
            </style>
          </object>
        </property>
      </object>
    </property>
    <signal name="notify::parent" handler="notify_parent_cb" swapped="yes"/>
    <style>
      <class name="menu"/>
    </style>
  </template>
</interface>
