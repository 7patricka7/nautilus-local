#.
#: libnautilus-extensions/nautilus-file.c:1922
msgid "yesterday %-I:%M %p"
msgstr "i går kl %H:%M"

#. Current week, include day of week.
#. * Note to localizers: Same issues as "today" string.
#.
#: libnautilus-extensions/nautilus-file.c:1927
msgid "%A %-m/%-d/%y %-I:%M %p"
msgstr "%A %Y-%-m-%-d %H:%M"

#. Other dates.
